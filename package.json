{
  "name": "auth0-extension-express-tools",
  "version": "0.8.0",
  "description": "A set of tools and utilities to simplify the development of Auth0 Extensions with Express.",
  "main": "src/index.js",
  "dependencies": {
    "auth0-extension-tools": "^1.0.0",
    "express": "^4.12.4",
    "express-jwt": "^3.1.0",
    "jwks-rsa": "^1.1.1",
    "webtask-tools": "^3.1.1"
  },
  "devDependencies": {
    "autochecker": "^0.9.2",
    "eslint": "^3.9.1",
    "eslint-config-auth0-base": "^6.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "faucet": "0.0.1",
    "nock": "^9.0.2",
    "nyc": "^8.4.0",
    "tape": "^4.6.2"
  },
  "scripts": {
    "release": "git tag $npm_package_version && git push --tags && npm publish",
    "test": "./node_modules/tape/bin/tape tests/**/*.js | ./node_modules/faucet/bin/cmd.js",
    "test:docker": "./node_modules/autochecker/cli.js 4.4.5 6.3.1",
    "test:coverage": "./node_modules/nyc/bin/nyc.js ./node_modules/tape/bin/tape tests/**/*.js | ./node_modules/faucet/bin/cmd.js && ./node_modules/nyc/bin/nyc.js report",
    "prepublish": "npm run test:docker"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0-extensions/auth0-extension-express-tools.git"
  },
  "keywords": [
    "auth0",
    "extensions",
    "express",
    "webtask"
  ],
  "author": "Auth0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0-extensions/auth0-extension-express-tools/issues"
  },
  "homepage": "https://github.com/auth0-extensions/auth0-extension-express-tools#readme"
}
